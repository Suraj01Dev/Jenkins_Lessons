currentBuild.displayName = "DockerBuildPush-#"+currentBuild.number

pipeline
{
    agent{
        label "jenkins-ssh-agent-docker"
    }
    stages{
        stage("Welcome")
        {
            steps{
            sh 'echo "Welcome" '
            }
        }

        stage("Docker Build")
        {
            steps{
                sh 'sudo docker build -t hello .'
    
            }
        }

    }
}
