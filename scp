pipeline{
    agent any;
    stages{
        stage("Sample File Creation")
        {
        steps{
            sh '''

                touch transfer.py
            '''
            }
        }
        stage("Using SCP")
        {
            steps{
                sshagent(['testvm']) {
                    sh'''

                    scp -o StrictHostKeyChecking=no transfer.py suraj@2fb8dcfa311c:
                    '''

                }
            }
        }
    }
}